{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "dialog:default",
    "shell:default",
    "fs:default",
    "fs:read-files",
    "fs:allow-mkdir",
    "fs:allow-read-dir",
    "fs:allow-download-write-recursive",
    "shell:allow-open"
  ]
}
